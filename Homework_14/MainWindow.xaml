<Window x:Class="Homework_14.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Homework_14"
        mc:Ignorable="d"
        Title="MainWindow" Height="250" Width="200">
    <Grid>
        <StackPanel HorizontalAlignment="Center">
            <Label Content="Выберите аккаунт"></Label>
            <Label Content=""></Label>
            <ComboBox x:Name="Employee">
                <TextBlock Text="Consultant"></TextBlock>
                <TextBlock Text="Manager"></TextBlock>
            </ComboBox>
            <Label Content=""></Label>
            <Button x:Name="OK" Height="25" Width="70" Content="OK" Click="OK_Click"></Button>
        </StackPanel>
    </Grid>
</Window>
